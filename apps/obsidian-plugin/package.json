{
  "name": "@digital-empire/obsidian-plugin",
  "version": "1.0.0",
  "description": "Sync Obsidian notes to your website via GitHub",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "keywords": ["obsidian", "sync", "github", "blog"],
  "author": "Benjamin Shafii",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.24.0",
    "obsidian": "latest",
    "tslib": "^2.8.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
